.ex-loader-airplane
  &:after
    content ""
    background-image url(/images/icons/preloader.svg)
    background-repeat no-repeat
    background-size cover
    display block
    position absolute
    top 50%
    left 50%
    margin-left -35px
    margin-top -35px
    width 70px
    height 77px
    animation-name rotation
    animation-duration: 1.5s;
    animation-iteration-count: infinite;
    animation-timing-function: cubic-bezier(0.175, 0.885, 0.32, 1.275)//cubic-bezier(0.6, -0.28, 0.74, 0.05);

    @-webkit-keyframes rotation
      from 
        transform rotate(0deg)
      to
        transform rotate(-360deg)
    @keyframes rotation
      from 
        transform rotate(0deg)
      to
        transform rotate(-360deg)